{
    "name": "se7en-ru/check24-task",
    "type": "project",
    "description": "Test rest task.",
    "keywords": ["test", "task", "rest"],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Yury Kudryavtsev"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.9"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
